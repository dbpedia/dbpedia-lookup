{
    // Use IntelliSense to learn about possible attributes.
    // Hover to view descriptions of existing attributes.
    // For more information, visit: https://go.microsoft.com/fwlink/?linkid=830387
    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch JettyLookupServer",
            "request": "launch",
            "mainClass": "org.dbpedia.lookup.JettyLookupServer",
            "projectName": "lookup-servlet",
            "args": "-c ./examples/servlet-config.yml -h ./examples"
        },
        {
            "type": "java",
            "name": "Launch Current File",
            "request": "launch",
            "mainClass": "${file}",
            "args": "-c ./examples/index-config.yml -"
        },
        {
            "type": "java",
            "name": "Launch Main",
            "request": "launch",
            "mainClass": "org.dbpedia.lookup.Main",
            "projectName": "lookup-indexer",
            "args": "-c ./examples/index-config.yml"
        }
    ]
}