    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Launch Jetty Lookup Server",
            "request": "launch",
            "mainClass": "org.dbpedia.lookup.JettyLookupServer",
            "projectName": "lookup-searcher",
            "args": "-c ./examples/search-config.yml"
        },
        {
            "type": "java",
            "name": "Launch Index Main",
            "request": "launch",
            "mainClass": "org.dbpedia.lookup.Main",
            "projectName": "lookup-indexer",
            "args": "-c ./examples/index-config.yml"
        },
        {
            "type": "java",
            "name": "Launch Index Main with Resource",
            "request": "launch",
            "mainClass": "org.dbpedia.lookup.Main",
            "projectName": "lookup-indexer",
            "args": "-c ./examples/index-config.yml -r 'http://dbpedia.org/resource/Arcadiopolis_in_Asia http://dbpedia.org/resource/Anaea_(Asia) http://dbpedia.org/resource/Puerto_As√≠s http://dbpedia.org/resource/Rockville,_Iowa'"
        }
    ]
}